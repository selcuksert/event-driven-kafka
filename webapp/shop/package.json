    "@webcomponents/webcomponentsjs": "^2.4.3",
    "lit-element": "^2.3.1",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "mocha": "^5.2.0",
    "polymer-cli": "^1.9.11",
